function makeCommit(){
  i = addressMap[eth.coinbase]
  unlock()
  var sec = web3.sha3(sks[i],1)
  var poly = []
  var temp = sec
  for (j=0;j<t;j++){
    poly.push(temp)
    temp = web3.sha3(temp)
  }
  var w = web3.sha3(sks[i],2)
  for (j=0;j<n;j++){
    var eva = con.evaluatePoly.call(poly,i+1)
    var bundle = con.computeShareAndProof.call(eva,pks[j].x,pks[j].y,w)
    console.log('PVSS : '+bundle)
    var e = bundle[0].toString(10)
    var z = bundle[1].toString(10)
    var pows = [bundle[2].toString(10),bundle[3].toString(10),bundle[4].toString(10),bundle[5].toString(10)]
    con.share.sendTransaction(j+1,bundle[0],bundle[1],[bundle[2],bundle[3],bundle[4],bundle[5]],{from:eth.coinbase,value:0})
  }
  var comm = web3.sha3(sec).toString(10)
  con.commit.sendTransaction(comm,{from:eth.coinbase,value:0})
  console.log(comm)
}
function listenAll(){
  var listeners = [con.RegisterReady(), con.Decryption(), con.CommitReady(), con.OpenReady(), con.PublicKey(), con.Share(), con.Commit(), con.Open()]
  listeners[0].watch(function(err, result) {  
      if (err) {    
          console.log(err)    
          return;  
      }  
      console.log('Event : Register Ready')
      reg()
  })
  listeners[1].watch(function(err, result) {
    if (err) {
      console.log(err)
      return;
    }
    console.log('Event : Decryption\n'+result.arg.decrypterAddress+','+result.args.sharerIndex+','+result.args.e+','+result.args.z+','+result.args.x+','+result.args.y)
  })
  listeners[2].watch(function(err, result) {
    if (err) {
      console.log(err)
      return;
    }
    console.log('Event : Commit Ready')
  })
  listeners[3].watch(function(err, result) {
    if (err) {
      console.log(err)
      return;
    }
    console.log('Event : Open Ready')
  })
  listeners[4].watch(function(err, result) {
    if (err) {
      console.log(err)
      return;
    }
    console.log('Event : Public Key\n'+result.args.index+','+result.args.partyAddress+','+result.args.x+','+result.args.y);
    addressMap[result.args.partyAddress] = result.args.index;
    pks.push({'x':result.args.x, 'y': result.args.y});
  })
  listeners[5].watch(function(err, result) {
    if (err) {
      console.log(err)
      return;
    }
    console.log('Event : Share\n'+result.args.shareIndex+','+result.args.partyAddress+','+result.args.e+','+result.args.z+','+result.args.shx+','+result.args.shy+','+result.args.vx+','+result.args.vy)
  })
  listeners[6].watch(function(err, result) {
    if (err) {
      console.log(err)
      return;
    }
    console.log('Event : Commit\n'+result.args.partyAddress+','+result.args.comm)
  })
  listeners[7].watch(function(err, result) {
    if (err) {
      console.log(err)
      return;
    }
    console.log('Event : Open\n'+result.args.partyAddress+','+result.args.x+','+result.arg.y)
  })
}
